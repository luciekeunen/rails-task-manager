<% @tasks.each do |task| %>
  <p><%= task.title %></p>
  <ul>
    <li><%= link_to "See task", task_path(task) %></li>
    <li><%= link_to "Edit task", edit_task_path(task) %></li>
    <li><%= link_to "Delete task", task_path(task), method: :delete %></li>
  </ul>
<% end %>

<%= link_to "Create new task", new_task_path %>
